a = input().split(',')
d = int(input())
b = []
c = []

for i in range(len(a)):
    b.append(0)

a = tuple(a)
cnt = 0

for i in range(len(a)-1):
    for j in range(i+1, len(a)):
        if a[i] == a[j]:
            b[j] = 1

for i in range(len(b)):
    if b[i] == 0:
        c.append(a[i])

for i in range(len(c)-1):
    for j in range(i+1, len(c)):
        if int(c[i]) + int(c[j]) == d:
            cnt += 1

print(cnt)
