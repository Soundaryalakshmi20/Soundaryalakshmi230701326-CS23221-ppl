def find_factor(n, p):
    factors = []
    for i in range(1, int(n**0.5) + 1):
        if n % i == 0:
            factors.append(i)
            if i != n // i:
                factors.append(n // i)
    factors.sort()
    if p <= len(factors):
        return factors[p-1]
    else:
        return 0

# Example usage
n = int(input())
p = int(input())
result = find_factor(n, p)
print(result)  
